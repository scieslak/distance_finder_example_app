<div class="row">
  <div class="small-offset-1 large-offset-3 large-6 small-10 columns">
    <div class="row">
      <div class="small-12">
        <%= form_tag({controller: "public_pages", action: "calculate"}, method: "post") do %>
          <%= text_field_tag 'origin', nil, id: "origin", placeholder: "origin" %>
          <%= text_field_tag :destination, nil, id: "destination", placeholder: "destination" %>
          <%= submit_tag "Calculate", class: "button large expanded" %>
        <% end %>
      </div>
    </div>

  </div>
  <div class="small-1 medium-3 columns"></div>

</div>


